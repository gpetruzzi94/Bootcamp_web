@using CodigoComun.Entities
@model IEnumerable<Stock>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Soy el Index de Stocks!</h1>

<a class="btn btn-dark" asp-action="Create">Agregar</a> 
<table class="table table-dark table-striped table-bordered border-white">
    <thead>
        <tr>
            <th>
                Id:
            </th>
            <th>
                IdArticulo:
            </th>
            <th>
                IdDeposito:
            </th>
            <th>
                Cantidad:
            </th>
            <th>
                Art_Nombre:
            </th>
            <th>
                Art_Codigo:
            </th>
            <th>
                Dep_Nombre:
            </th>
            <th>
            Opciones:
            </th>

        </tr>
    <thead />
    <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.Id</td>
                <td>@item.IdArticulo</td>
                <td>@item.IdDeposito</td>
                <td>@item.Cantidad</td>
                <td>@item.IdArticuloNavigation.Nombre</td>
                <td>@item.IdArticuloNavigation.Codigo</td>
                <td>@item.IdDepositoNavigation.Nombre</td>
                <td>
                    <a class ="btn btn-primary" asp-action="Edit" asp-route-id="@item.Id">Editar</a>
                    <a class ="btn btn-danger" asp-action="Delete" asp-route-id="@item.Id">Eliminar</a>
                </td>
            </tr>


            }
    </tbody>

<table />
